{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "indent": "off",
        "eol-last": ["error", "never"],
        "class-methods-use-this": "off",
        "space-before-blocks": "off"
    }
}
